import java.util.*;
class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        int largNum=0;
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
            
            if(arr[i]>largNum){
               largNum=arr[i]; 
            }
        }
        System.out.println(largNum);
    }
}

//Output:
5
1 6 5 9 3
9
